window/size/viewport_height=324

[editor_plugins]

enabled=PackedStringArray("res://addons/protobuf/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
